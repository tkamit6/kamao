{
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/$1",
        "headers": {
          "cache-control": "s-maxage=31536000, immutable"
        }
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  